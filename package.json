{
  "name": "@arthur.lee945/promise-pool",
  "version": "1.0.1",
  "description": "Queue based promise pool",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist"
  ],
  "main": "dist",
  "types": "dist",
  "homepage": "https://github.com/arthurlee945/promise-pool",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthurlee945/promise-pool.git"
  },
  "bugs": {
    "url": "https://github.com/arthurlee945/promise-pool/issues"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:performance": "ts-node ./src/test/performance.ts",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint --fix src --ext .js,.ts",
    "format": "prettier --write src",
    "clean": "rm -rf node_modules pnpm-lock.yaml",
    "prepare": "husky install",
    "prepublishOnly" : "pnpm build"
