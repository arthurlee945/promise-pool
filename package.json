{
    "name": "promise-pool",
    "version": "0.0.1",
    "description": "Queue based promise pool",
    "files": [
        "dist"
    ],
    "main": "dist",
    "types": "dist",
    "scripts": {
        "build": "tsc",
        "start": "ts-node ./src/test.ts",
        "test": "jest",
        "lint": "eslint src --ext .js,.ts",
        "lint:fix": "eslint --fix src --ext .js,.ts",
        "format": "prettier --write src",
        "clean": "rm -rf node_modules pnpm-lock.yaml",
        "prepare": "husky install"
    },
    "keywords": [
        "promises",
        "nodejs",
        "async",
        "map"
    ],
    "author": "arthur lee",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.23.5",
        "@babel/preset-env": "^7.23.5",
        "@babel/preset-typescript": "^7.23.3",
        "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
        "@supercharge/promise-pool": "^3.1.0",
        "@types/eslint": "^8.44.7",
        "@types/jest": "^29.5.10",
        "@types/node": "^20.10.0",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "babel-jest": "^29.7.0",
        "eslint": "^8.54.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.29.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "prettier": "^3.1.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.2"
    }
}
